<project>
  <title>Iterated integrals</title>
  <sage>
  <input>
  f(x,y)=x+y^2
  a=0
  b=1
  c=0
  d=1
  show(integrate(f(x,y),x,0,1))
  show(integrate(f(x,y),y,0,1))
  show(integrate(integrate(f(x,y),x,0,1),y,0,1))
  show(integrate(integrate(f(x,y),y,0,1),x,0,1))
